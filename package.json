{
  "name": "caravago",
  "version": "1.0.0",
  "description": "CaravaGo - Full Stack Application",
  "scripts": {
    "dev": "pnpm exec concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:backend": "cd backend && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "install:all": "pnpm install && cd backend && pnpm install && cd ../frontend && pnpm install",
    "build:frontend": "cd frontend && pnpm run build",
    "start:backend": "cd backend && pnpm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["caravago", "fullstack", "react", "supabase"],
  "author": "CaravaGo Team",
  "license": "MIT"
}